{
  "name": "apptimists/laravel-spatial",
  "description": "Spatial data types extension for Laravel.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Johannes Berdin",
      "email": "ich@johannesberdin.de"
    }
  ],
  "require": {
    "php": ">=5.6.4",
    "doctrine/dbal": "^2.5",
    "funiq/geophp": "dev-master",
    "illuminate/database": "^5.3",
    "jmikola/geojson": "^1.0"
  },
  "require-dev": {
    "laravel/browser-kit-testing": "^2.0",
    "laravel/laravel": "^5.3",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~5.0"
  },
  "autoload": {
    "psr-4": {
      "LaravelSpatial\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/Unit",
      "tests/Integration"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
