{
  "name": "apptimists/laravel-spatial",
  "description": "Spatial data types extension for Laravel.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Johannes Berdin",
      "email": "ich@johannesberdin.de"
    }
  ],
  "require": {
    "php": ">=5.6.4",
    "illuminate/database": "^5.3",
    "jmikola/geojson": "^1.0",
    "phayes/geophp": "^1.2"
  },
  "require-dev": {
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~5.0"
  },
  "autoload": {
    "psr-4": {
      "LaravelSpatial\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "LaravelSpatial\\SpatialServiceProvider"
      ]
    },
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
